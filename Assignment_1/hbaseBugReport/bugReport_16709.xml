./hbase-client/pom.xml
      &amp;lt;id&amp;gt;hadoop-1.1&amp;lt;/id&amp;gt;
./hbase-common/pom.xml
         &amp;lt;id&amp;gt;hadoop-1.1&amp;lt;/id&amp;gt;
./hbase-examples/pom.xml
      &amp;lt;id&amp;gt;hadoop-1.1&amp;lt;/id&amp;gt;
./hbase-external-blockcache/pom.xml
      &amp;lt;id&amp;gt;hadoop-1.1&amp;lt;/id&amp;gt;
./hbase-it/pom.xml
      &amp;lt;id&amp;gt;hadoop-1.1&amp;lt;/id&amp;gt;
./hbase-prefix-tree/pom.xml
      &amp;lt;id&amp;gt;hadoop-1.1&amp;lt;/id&amp;gt;
./hbase-procedure/pom.xml
      &amp;lt;id&amp;gt;hadoop-1.1&amp;lt;/id&amp;gt;
./hbase-server/pom.xml
      &amp;lt;id&amp;gt;hadoop-1.1&amp;lt;/id&amp;gt;
./hbase-shell/pom.xml
            &amp;lt;id&amp;gt;hadoop-1.1&amp;lt;/id&amp;gt;
./hbase-testing-util/pom.xml
      &amp;lt;id&amp;gt;hadoop-1.1&amp;lt;/id&amp;gt;
./hbase-thrift/pom.xml
&lt;/pre&gt;
&lt;/div&gt;&lt;/div&gt;
&lt;p&gt;hadoop-1.1 profile can be dropped in the above pom.xml for hbase 2.0&lt;/p&gt;</description>
