+    count = scanTable(table, rowa, rowd);
+    metricsRegionServer.getRegionServerWrapper().forceRecompute();
+    assertEquals(count, 2);
+    &lt;span class=&quot;code-comment&quot;&gt;// It is weired! The deleted rowb not calculated!!
&lt;/span&gt;+    metricsHelper.assertCounter(&lt;span class=&quot;code-quote&quot;&gt;&quot;readRequestCount&quot;&lt;/span&gt;, readRequests + 6, serverSource);
