&lt;pre class=&quot;code-java&quot;&gt;
    Cell clonedBaseCell = ((ExtendedCell) &lt;span class=&quot;code-keyword&quot;&gt;this&lt;/span&gt;.cell).deepClone();
822	      &lt;span class=&quot;code-keyword&quot;&gt;if&lt;/span&gt; (clonedBaseCell &lt;span class=&quot;code-keyword&quot;&gt;instanceof&lt;/span&gt; ByteBufferCell) {
