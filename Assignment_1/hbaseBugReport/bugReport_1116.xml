+++ build.xml   (working copy)
@@ -174,7 +174,7 @@
    &amp;lt;!--Conditionally generate the jsp java pages.
        We &lt;span class=&quot;code-keyword&quot;&gt;do&lt;/span&gt; it once per ant invocation.  See hbase-593.
      --&amp;gt;
-  &amp;lt;target name=&lt;span class=&quot;code-quote&quot;&gt;&quot;jspc&quot;&lt;/span&gt; unless=&lt;span class=&quot;code-quote&quot;&gt;&quot;jspc.not.required&quot;&lt;/span&gt;&amp;gt;
