       &amp;lt;fileset dir=&lt;span class=&quot;code-quote&quot;&gt;&quot;${basedir}/lib/jetty-ext/&quot;&lt;/span&gt;&amp;gt;
         &amp;lt;include name=&lt;span class=&quot;code-quote&quot;&gt;&quot;*jar&quot;&lt;/span&gt; /&amp;gt;
@@ -191,13 +191,13 @@
      uriroot=&lt;span class=&quot;code-quote&quot;&gt;&quot;${src.webapps}/master&quot;&lt;/span&gt;
      outputdir=&lt;span class=&quot;code-quote&quot;&gt;&quot;${generated.webapps.src}&quot;&lt;/span&gt;
