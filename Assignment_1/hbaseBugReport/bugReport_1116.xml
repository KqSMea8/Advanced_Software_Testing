     &amp;lt;property name=&lt;span class=&quot;code-quote&quot;&gt;&quot;jspc.not.required&quot;&lt;/span&gt; value=&lt;span class=&quot;code-quote&quot;&gt;&quot;&lt;span class=&quot;code-keyword&quot;&gt;true&lt;/span&gt;&quot;&lt;/span&gt; /&amp;gt;
