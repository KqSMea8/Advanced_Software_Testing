&lt;p&gt;in 0.98.5 SimpleRpcScheduler#SimpleRpcScheduler:&lt;/p&gt;
&lt;div class=&quot;code panel&quot; style=&quot;border-width: 1px;&quot;&gt;&lt;div class=&quot;codeContent panelContent&quot;&gt;
&lt;pre class=&quot;code-java&quot;&gt;
     &lt;span class=&quot;code-object&quot;&gt;int&lt;/span&gt; maxQueueLength = conf.getInt(CALL_QUEUE_MAX_LENGTH_CONF_KEY,
      conf.getInt(&lt;span class=&quot;code-quote&quot;&gt;&quot;ipc.server.max.callqueue.length&quot;&lt;/span&gt;,
        handlerCount * RpcServer.DEFAULT_MAX_CALLQUEUE_LENGTH_PER_HANDLER));
  
    &lt;span class=&quot;code-object&quot;&gt;float&lt;/span&gt; callqReadShare = conf.getFloat(CALL_QUEUE_READ_SHARE_CONF_KEY,
      conf.getFloat(&lt;span class=&quot;code-quote&quot;&gt;&quot;ipc.server.callqueue.read.share&quot;&lt;/span&gt;, 0));

    &lt;span class=&quot;code-object&quot;&gt;float&lt;/span&gt; callQueuesHandlersFactor = conf.getFloat(CALL_QUEUE_HANDLER_FACTOR_CONF_KEY,
      conf.getFloat(&lt;span class=&quot;code-quote&quot;&gt;&quot;ipc.server.callqueue.handler.factor&quot;&lt;/span&gt;, 0));
&lt;/pre&gt;
&lt;/div&gt;&lt;/div&gt;</description>
