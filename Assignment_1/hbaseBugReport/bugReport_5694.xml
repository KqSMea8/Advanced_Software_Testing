+          get.setTimestamp(timestamp);
           Result result = table.get(get);
           &lt;span class=&quot;code-keyword&quot;&gt;return&lt;/span&gt; ThriftUtilities.rowResultFromHBase(result);
         }
@@ -426,7 +426,7 @@ &lt;span class=&quot;code-keyword&quot;&gt;public&lt;/span&gt; class ThriftServer {
               get.addColumn(famAndQf[0], famAndQf[1]);
           }
         }
-        get.setTimeRange(&lt;span class=&quot;code-object&quot;&gt;Long&lt;/span&gt;.MIN_VALUE, timestamp);
+        get.setTimestamp(timestamp);
         Result result = table.get(get);
         &lt;span class=&quot;code-keyword&quot;&gt;return&lt;/span&gt; ThriftUtilities.rowResultFromHBase(result);
       } &lt;span class=&quot;code-keyword&quot;&gt;catch&lt;/span&gt; (IOException e) {
&lt;/pre&gt;
