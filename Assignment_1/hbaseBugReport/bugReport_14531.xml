+  &lt;span class=&quot;code-keyword&quot;&gt;if&lt;/span&gt; [ &lt;span class=&quot;code-quote&quot;&gt;&quot;$local&quot;&lt;/span&gt; ==@#^&amp;amp; &lt;span class=&quot;code-keyword&quot;&gt;true&lt;/span&gt; ]; then
     &lt;span class=&quot;code-quote&quot;&gt;&quot;$bin&quot;&lt;/span&gt;/hbase-daemon.sh --config ${HBASE_CONF_DIR} stop thrift
