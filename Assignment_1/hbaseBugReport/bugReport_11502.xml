book.xml:              fileref=&quot;hbase_logo.png&quot; /&amp;gt;&lt;br/&gt;
book.xml:                fileref=&quot;timeline_consistency.png&quot; /&amp;gt;&lt;br/&gt;
book.xml:                   &amp;lt;imagedata align=&quot;center&quot; valign=&quot;middle&quot; fileref=&quot;hfile.png&quot; /&amp;gt;&lt;br/&gt;
book.xml:                   &amp;lt;imagedata align=&quot;center&quot; valign=&quot;middle&quot; fileref=&quot;hfilev2.png&quot; /&amp;gt;&lt;br/&gt;
book.xml.orig:              fileref=&quot;hbase_logo.png&quot; /&amp;gt;&lt;br/&gt;
book.xml.orig:	                &amp;lt;imagedata align=&quot;middle&quot; valign=&quot;middle&quot; fileref=&quot;timeline_consistency.png&quot; /&amp;gt;&lt;br/&gt;
