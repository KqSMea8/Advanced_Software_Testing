./hbase-thrift/src/main/resources/hbase-webapps/&lt;span class=&quot;code-keyword&quot;&gt;static&lt;/span&gt;/css/bootstrap.css
./hbase-thrift/src/main/resources/hbase-webapps/&lt;span class=&quot;code-keyword&quot;&gt;static&lt;/span&gt;/css/bootstrap.min.css
./hbase-thrift/src/main/resources/hbase-webapps/&lt;span class=&quot;code-keyword&quot;&gt;static&lt;/span&gt;/js/bootstrap.js
