	at com.intellij.psi.impl.source.parsing.xml.XmlBuilderDriver.build(XmlBuilderDriver.java:74)
	at org.jetbrains.idea.maven.utils.MavenJDOMUtil.doRead(MavenJDOMUtil.java:128)
