                            <comment id="12709010" author="stack" created="Wed, 13 May 2009 17:08:36 +0000"  >&lt;p&gt;I did this:&lt;/p&gt;

&lt;div class=&quot;code panel&quot; style=&quot;border-width: 1px;&quot;&gt;&lt;div class=&quot;codeContent panelContent&quot;&gt;
&lt;pre class=&quot;code-java&quot;&gt;
Index: src/java/org/apache/hadoop/hbase/ipc/HBaseClient.java
===================================================================
--- src/java/org/apache/hadoop/hbase/ipc/HBaseClient.java	(revision 771545)
+++ src/java/org/apache/hadoop/hbase/ipc/HBaseClient.java	(working copy)
@@ -202,8 +202,8 @@
       &lt;span class=&quot;code-keyword&quot;&gt;this&lt;/span&gt;.remoteId = remoteId;
       UserGroupInformation ticket = remoteId.getTicket();
       &lt;span class=&quot;code-keyword&quot;&gt;this&lt;/span&gt;.setName(&lt;span class=&quot;code-quote&quot;&gt;&quot;IPC Client (&quot;&lt;/span&gt; + socketFactory.hashCode() +&lt;span class=&quot;code-quote&quot;&gt;&quot;) connection to &quot;&lt;/span&gt; +
-          remoteId.getAddress().toString() +
-          &lt;span class=&quot;code-quote&quot;&gt;&quot; from &quot;&lt;/span&gt; + ((ticket==&lt;span class=&quot;code-keyword&quot;&gt;null&lt;/span&gt;)?&lt;span class=&quot;code-quote&quot;&gt;&quot;an unknown user&quot;&lt;/span&gt;:ticket.getUserName()));
+        remoteId.getAddress().toString() +
+        ((ticket==&lt;span class=&quot;code-keyword&quot;&gt;null&lt;/span&gt;)?&lt;span class=&quot;code-quote&quot;&gt;&quot; from an unknown user&quot;&lt;/span&gt;: (&lt;span class=&quot;code-quote&quot;&gt;&quot; from &quot;&lt;/span&gt; + ticket.getUserName())));
       &lt;span class=&quot;code-keyword&quot;&gt;this&lt;/span&gt;.setDaemon(&lt;span class=&quot;code-keyword&quot;&gt;true&lt;/span&gt;);
     }
&lt;/pre&gt;
